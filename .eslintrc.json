{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len":"warn",
        "no-console":"off",
        "arrow-spacing":"off",
        "arrow-parens":"off",
        "spaced-comment":"off",
        "space-in-parens":"off",
        "no-unused-vars":"warn",
        "no-unused-expressions":"warn",
        "no-param-reassign":"off",
        "no-plusplus":"off",
        "no-shadow":"warn",
        "comma-dangle":"off",
        "no-unneeded-ternary":"warn",
        "default-case":"off",
        "padded-blocks":"off"
    }
}
