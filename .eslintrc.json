{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len":"warn",
        "no-console":"off",
        "arrow-spacing":"off",
        "arrow-parens":"off",
        "spaced-comment":"off",
        "space-in-parens":"warn",
        "no-unused-vars":"warn",
        "no-shadow":"warn",
        "comma-dangle":"off",
        "no-unneeded-ternary":"warn"
    }
}
