{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "import/extensions": "off",
        "max-len":"warn",
        "no-console":"off",
        "arrow-spacing":"off",
        "computed-property-spacing":"off",
        "spaced-comment":"off",
        "space-in-parens":"off",
        "no-unused-vars":"warn",
        "one-var":"warn",
        "no-unused-expressions":"warn",
        "no-param-reassign":"off",
        "no-plusplus":"off",
        "no-shadow":"warn",
        "comma-dangle":"off",
        "no-unneeded-ternary":"warn",
        "default-case":"off",
        "padded-blocks":"off",
        "consistent-return":"off",
        "template-curly-spacing":"off"
    }
}
